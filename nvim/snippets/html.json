{
  "Jinja2 Block": {
    "prefix": "%%",
    "body": [
      "{% ${1} %}"
    ]
  },
  "Jinja2 For": {
    "prefix": "%for",
    "body": [
      "{% for ${1} in ${2} %}",
      "${3}",
      "{% endfor %}"
    ]
  },
  "Jinja2 If": {
    "prefix": "%if",
    "body": [
      "{% if ${1} %}",
      "${2}",
      "{% endif %}"
    ]
  }
}
